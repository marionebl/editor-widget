{
  "name": "react-blessed-editor",
  "version": "0.0.0",
  "description": "<Editor> component for react-blessed",
  "homepage": "https://github.com/marionebl/react-blessed-editor",
  "bugs": "https://github.com/marionebl/react-blessed-editor/issues",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run test -- --watch",
    "build": "rm -rf ./dist && babel ./src --out-dir ./dist",
    "test": "eslint src && ava",
    "examples:basic": "babel-live ./src/examples/basic.js",
    "examples:input": "babel-live ./src/examples/input.js",
    "examples:basic:fastboot": "node dist/examples/basic.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marionebl/react-blessed-editor.git"
  },
  "dependencies": {
    "@marionebl/redux-cli-logger": "^1.4.0",
    "autobind-decorator": "^1.3.3",
    "blessed": "0.1.81",
    "bluebird": "3.1.1",
    "emphasize": "^1.0.0",
    "lodash": "^4.13.1",
    "pure-render-decorator": "^1.1.1",
    "rc": "1.1.6",
    "react-is-deprecated": "^0.1.2",
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "@marionebl/babel-live": "^1.4.0",
    "ava": "^0.15.2",
    "babel-cli": "6.3.17",
    "babel-eslint": "^6.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "enzyme": "^2.3.0",
    "eslint": "^3.0.1",
    "eslint-config-xo": "^0.15.2",
    "eslint-config-xo-react": "^0.8.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-react": "^5.2.2",
    "get-random-port": "0.0.1",
    "react": "0.14.x",
    "react-addons-test-utils": "^0.14.8",
    "react-blessed": "0.1.x",
    "react-dom": "^0.14.8",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "tape": "4.4.0",
    "unexpected": "^10.14.2",
    "unexpected-react": "^3.2.3"
  },
  "peerDependencies": {
    "react": "0.14.x",
    "react-blessed": "0.1.x"
  },
  "author": "Mario Nebl <hello@herebecode.com>",
  "license": "MIT",
  "ava": {
    "files": [
      "test/**/*.js",
      "!helpers/**/*"
    ],
    "source": [
      "src/**/*.js"
    ],
    "concurrency": 4,
    "failFast": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo/esnext",
      "xo-react"
    ]
  }
}
